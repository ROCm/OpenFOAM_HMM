FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 144.87; // epsilon/Cmu/k

boundaryField
{
    "(bottom|top)"
    {
        type            omegaWallFunction;
        value           $internalField;
    }

    "(inlet|outlet)"
    {
        type            cyclic;
    }

    leftAndRight
    {
        type            empty;
    }
}


// ************************************************************************* //
