FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    bottom
    {
        type            zeroGradient;
    }

    top
    {
        type            symmetry;
    }

    inlet
    {
        type            fixedValue;
        value           uniform 1.0;
    }

    outlet
    {
        type            fixedValue;
        value           uniform 0.0;
    }

    leftAndRight
    {
        type            empty;
    }
}


