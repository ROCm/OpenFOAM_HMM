FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue DT 1
);

regions
(
    cylinderToCell
    {
        p1 (0 0 -5);
        p2 (0 0 0);
        radius 1;
        fieldValues
        (
            volScalarFieldValue DT 1e6
        );
    }
);


