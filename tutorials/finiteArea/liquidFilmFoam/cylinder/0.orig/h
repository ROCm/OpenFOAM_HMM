FoamFile
{
    version     2.0;
    format      ascii;
    class       areaScalarField;
    object      h;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 1 0 0 0 0 0];

internalField       uniform 0.000141;

boundaryField
{
    inlet
    {
        type         fixedValue;
        value        uniform 0.000141;
    }

    outlet
    {
        type         zeroGradient;
    }

    side
    {
        type        zeroGradient;
    }

    symmetry
    {
        type        symmetry;
    }

    cylinder
    {
        type        zeroGradient;
    }
}


