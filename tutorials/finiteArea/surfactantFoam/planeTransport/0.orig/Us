FoamFile
{
    version     2.0;
    format      ascii;
    class       areaVectorField;
    object      Us;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 1 -1 0 0 0 0];

internalField       uniform (0.05 0 0);

boundaryField
{
    inlet1
    {
        type        fixedValue;
        value       $internalField;
    }

    inlet2
    {
        type        fixedValue;
        value       $internalField;
    }

    outlet
    {
        type        zeroGradient;
    }

    bound
    {
        type        symmetry;
    }
}


