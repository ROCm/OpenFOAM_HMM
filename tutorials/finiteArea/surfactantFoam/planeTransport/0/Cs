FoamFile
{
    version     2.0;
    format      ascii;
    class       areaScalarField;
    location    "0";
    object      Cs;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [1 -2 0 0 0 0 0];

internalField       uniform 0;

referenceLevel      0;

boundaryField
{
    inlet
    {
        type fixedValue;
        value uniform 1;
    }
    outlet
    {
        type inletOutlet;
        value uniform 1;
        phi phis;
        inletValue uniform 0;
    }
    bound
    {
        type symmetry;
    }
}

