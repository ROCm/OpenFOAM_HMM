FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1e-3;

boundaryField
{
    metalSheet
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    "side-.*"
    {
        type            inletOutlet;
        inletValue      uniform 1e-3;
        value           uniform 1e-3;
    }
}


