FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
    volVectorFieldValue U (0 0 0)
);

regions
(
    boxToCell
    {
        box ( -10.000 -10.000 -10.000 ) ( 250.000 250.000 0.2 );
        fieldValues ( volScalarFieldValue alpha.water 1 );
    }
);


