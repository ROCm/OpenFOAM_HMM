FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type            uniformFixedValue;
        uniformValue    table
                        (
                                (0      1)
                                (1      1)
                                (1.01   0)
                                (14     0)
                        );

    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    walls
    {
        type            zeroGradient;
    }
    base
    {
        type            zeroGradient;
    }

    defaultFaces
    {
        type            empty;
    }
}

