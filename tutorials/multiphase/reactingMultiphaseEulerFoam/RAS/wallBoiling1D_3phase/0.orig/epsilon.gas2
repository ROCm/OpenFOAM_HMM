FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon.gas2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.001;

boundaryField
{
    inlet
    {
        type            mapped;
        field           epsilon.gas;
        setAverage      0;
        average         0;
        interpolationScheme cell;
        value           uniform 0.01;
    }

    outlet
    {
        type            inletOutlet;
        phi             phi.gas;
        inletValue      uniform 0.001;
    }

    wall1
    {
        type            epsilonWallFunction;
        value           uniform 0.001;
    }

    wall2
    {
        type            epsilonWallFunction;
        value           uniform 0.001;
    }

    defaultFaces
    {
        type            empty;
    }
}


