FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;

    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type    massTransferMultiphaseSystem;

phases  (gas liquid);

liquid
{
    type            pureMovingPhaseModel;
}

gas
{
    type            multiComponentMovingPhaseModel;
}

surfaceTension
(
    (gas and liquid)
    {
        type            constant;
        sigma           0.00;
    }
);


massTransferModel
(
    (liquid to gas)
    {
        type                kineticGasEvaporation;
        species             vapour.gas;
        C                   0.1;
        isoAlpha            0.1;
        Tactivate           372;
   }
);


