FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 6.9e-5;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    "(atmosphere|outlet)"
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    "(HULL*.*)"
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    "(BLADES|HUB_DISK|HUB)"
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    "(overset-1|oversetPatch)"
    {
        type           overset;
    }
}


// ************************************************************************* //
