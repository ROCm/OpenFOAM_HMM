FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      dynamicMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

limitU
{
    type            velocityDampingConstraint;
    active          false;
    selectionMode   cellType;
    UMax            0;
    C               4;
}

limitUAll
{
    type            limitVelocity;
    active          false;
    selectionMode   all;
    max             0.2;
}


