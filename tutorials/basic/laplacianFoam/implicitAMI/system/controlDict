application     laplacianFoam;

startFrom       startTime;  //latestTime;

startTime       0;

stopAt          endTime;

endTime         1;

deltaT          1;

//writeControl    runTime;
//writeInterval   0.1;
writeControl    timeStep;
writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

