FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [m s^-1];

internalField   uniform (0 0 0);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    minX
    {
        type            uniformFixedValue;
        uniformValue    (1 0 0);
    }
    maxX
    {
        type            zeroGradient;
    }

    wall
    {
        type            uniformFixedValue;
        uniformValue    (0 0 0);
    }
}


