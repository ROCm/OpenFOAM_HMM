FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      stitchMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

outerx
{
    match   partial;    // partial | integral | perfect
    master  outerx;
    slave   innerx;
}

outery
{
    match   partial;
    master  outery;
    slave   innery;
}

outerz
{
    match   partial;
    master  outerz;
    slave   innerz;
}

