FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      rhoM;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }

    inlet
    {
        type            zeroGradient;
    }

    sides
    {
        type            zeroGradient;
    }
}

