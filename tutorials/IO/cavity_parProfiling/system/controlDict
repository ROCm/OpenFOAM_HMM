FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Can force loading of solver wrapper...
libs            (utilityFunctionObjects);

application     icoFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.1;

deltaT          0.005;

writeControl    timeStep;

writeInterval   20;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
    // Add some dummy time - shouldn't be seen in profiling
    sleep
    {
        type    coded;
        libs    (utilityFunctionObjects);
        name    sleep;

        codeExecute #{ sleep(1); #};
    }

    // Run parProfiling
    profiling
    {
        #includeEtc "caseDicts/profiling/parallel.cfg"
        detail  2;
    }
}

// ************************************************************************* //
