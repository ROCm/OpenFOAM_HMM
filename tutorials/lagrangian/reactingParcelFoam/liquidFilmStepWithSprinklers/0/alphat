FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 1e-3;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 1e-3;
    }

    outlet
    {
        type            calculated;
        value           uniform 1e-3;
    }

    ".*"
    {
        type            compressible::alphatWallFunction;;
        value           uniform 1e-3;
    }
}


