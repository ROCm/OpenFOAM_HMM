actions
(
    {
        name    f0;
        type    faceSet;
        action  new;
        source  patchToFace;
        patches (filmWalls);
    }

    {
        name    f0;
        type    faceSet;
        action  subset;
        source  boxToFace;
        min     (0.05 -100 -100);
        max     (100 100 100);
    }
);


