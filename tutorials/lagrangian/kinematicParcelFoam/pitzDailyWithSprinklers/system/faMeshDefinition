FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      faMeshDefinition;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

polyMeshPatches   ( base );

boundary
{
    inlet
    {
        type                patch;
        ownerPolyPatch      base;
        neighbourPolyPatch  inlet;
    }

    outlet
    {
        type                patch;
        ownerPolyPatch      base;
        neighbourPolyPatch  outlet;
    }

    sides
    {
        type                patch;
        ownerPolyPatch      base;
        neighbourPolyPatch  sides;
    }
}


