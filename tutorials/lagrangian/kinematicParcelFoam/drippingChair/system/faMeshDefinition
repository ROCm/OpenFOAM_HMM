FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      faMeshDefinition;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

polyMeshPatches   (filmWalls);

boundary
{
    top
    {
        type                patch;
        ownerPolyPatch      filmWalls;
        neighbourPolyPatch  top;
    }

    bottom
    {
        type                patch;
        ownerPolyPatch      filmWalls;
        neighbourPolyPatch  bottom;
    }

    frontAndBack
    {
        type                patch;
        ownerPolyPatch      filmWalls;
        neighbourPolyPatch  frontAndBack;
    }
}


