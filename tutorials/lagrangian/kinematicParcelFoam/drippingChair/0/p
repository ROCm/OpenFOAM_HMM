FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    top
    {
        type            zeroGradient;
    }

    bottom
    {
        type            totalPressure;
        p0              uniform 0;
    }

    "(filmWalls|sides|frontAndBack)"
    {
        type            zeroGradient;
    }
}


