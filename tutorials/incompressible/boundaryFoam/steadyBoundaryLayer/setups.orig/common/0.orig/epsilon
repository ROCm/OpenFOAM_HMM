FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1e-08;

boundaryField
{
    lowerWall
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    upperWall
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    front
    {
        type            cyclic;
    }

    back
    {
        type            cyclic;
    }

    defaultFaces
    {
        type            empty;
    }
}


// ************************************************************************* //
