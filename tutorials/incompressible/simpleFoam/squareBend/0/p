FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    Default_Boundary_Region
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
        refValue        $internalField;
        refGradient     uniform 0;
        valueFraction   uniform 0.3;
    }
    outlet
    {
        type            fixedValue;
        value           $internalField;
    }

    ".*"
    {
        type            zeroGradient;
    }
}


