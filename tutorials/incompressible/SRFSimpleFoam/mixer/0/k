FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.375;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.375;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.375;
    }

    innerWall
    {
        type            kqRWallFunction;
        value           uniform 0.375;
    }

    outerWall
    {
        type            kqRWallFunction;
        value           uniform 0.375;
    }

    cyclic_half0
    {
        type            cyclic;
        value           uniform 0.375;
    }

    cyclic_half1
    {
        type            cyclic;
        value           uniform 0.375;
    }
}


