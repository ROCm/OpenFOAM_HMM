FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (17.55 0 0);

boundaryField
{
    inlet
    {
        type            turbulentDigitalFilterInlet;
        fsm             true;
        n               ( 64 70 );
        L
        (
            0.04446467692 0.01771245128 0.01950205128
            0.172787596   0.171889998   0.224578995
            0.1728125     0.171875      0.22459375
        );
        mean
        {
            type        mappedFile;
            mapMethod   nearest;
            fieldTable  U;
        }
        R
        {
            type        mappedFile;
            mapMethod   nearest;
        }
        value           $internalField;
    }

    outlet
    {
        type            advective;
        value           $internalField;
    }

    "(bottom|top)"
    {
        type            noSlip;
    }

    "(left|right)"
    {
        type            cyclic;
    }
}


