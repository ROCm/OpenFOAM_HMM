FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        "system";
    object          sample;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type            sets;
libs            ("libsampling.so");
writeControl    writeTime;

interpolationScheme cellPoint;

setFormat raw;

sets
(
    xm10
    {
        type            face;
        axis            z;
        start           (2.5 4.5 0);
        end             (2.5 4.5 2);
    }
    xm05
    {
        type            face;
        axis            z;
        start           (3 4.5 0);
        end             (3 4.5 2);
    }
