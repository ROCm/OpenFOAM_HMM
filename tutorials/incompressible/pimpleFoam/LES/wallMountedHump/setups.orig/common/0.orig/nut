FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1.06837606838e-06;

boundaryField
{
    inlet
    {
        type            calculated;
        value           $internalField;
    }

    outlet
    {
        type            calculated;
        value           $internalField;
    }

    bottomWall
    {
        type            nutUSpaldingWallFunction;
        value           $internalField;
        tolerance       1e-9;
    }

    topWall
    {
        type            slip;
    }

    "zPeriodic_.*"
    {
        type            cyclic;
    }
}

// ************************************************************************* //
