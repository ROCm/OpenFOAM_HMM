FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -1 0 0 0 0 ];

internalField   uniform 2.5e-4;

boundaryField
{
    frontBack
    {
        type            empty;
    }

    suction
    {
        type            fixedValue;
        value           uniform 0;
    }

    pressure
    {
        type            fixedValue;
        value           uniform 0;
    }

    inlet
    {
        type            inletOutlet;
        inletValue      uniform 2.5e-04;
        value           uniform 2.5e-04;
    }
}


