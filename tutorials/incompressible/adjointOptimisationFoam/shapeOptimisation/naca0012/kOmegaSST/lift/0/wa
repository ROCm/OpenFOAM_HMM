FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      wa;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -1 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    frontBack
    {
        type            empty;
    }
    suction
    {
        type            waWallFunction;
        value           uniform 0;
    }
    pressure
    {
        type            waWallFunction;
        value           uniform 0;
    }
    inlet
    {
        type            adjointFarFieldTMVar2;
        value           $internalField;
    }
}

