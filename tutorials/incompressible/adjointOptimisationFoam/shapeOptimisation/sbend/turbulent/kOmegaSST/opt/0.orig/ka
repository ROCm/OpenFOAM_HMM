FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      ka;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    frontBack
    {
        type            empty;
    }
    sideWall
    {
        type            kaqRWallFunction;
        value           uniform 0;
    }
    upper
    {
        type            kaqRWallFunction;
        value           uniform 0;
    }
    lower
    {
        type            kaqRWallFunction;
        value           uniform 0;
    }
    Inlet
    {
        type            adjointZeroInlet;
        value           uniform 0;
    }
    Outlet
    {
        type            adjointOutletKa;
        value           uniform 0;
    }
}

