FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 0.0585;

boundaryField
{
    frontBack
    {
        type            empty;
    }
    sideWall
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    upper
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    lower
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    Inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    Outlet
    {
        type            zeroGradient;
    }
}

// ************************************************************************* //
