FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application       adjointOptimisationFoam;

startFrom         latestTime;

startTime         0;

stopAt            endTime;

endTime           5;

deltaT            1;

writeControl      timeStep;

writeInterval     1;

purgeWrite        0;

writeFormat       ascii;

writePrecision    16;

writeCompression  true;

timeFormat        general;

timePrecision     6;

runTimeModifiable yes;

functions
{
    yPlus
    {
        type            yPlus;
        libs            (fieldFunctionObjects);
        writeControl    writeTime;
    }
}


