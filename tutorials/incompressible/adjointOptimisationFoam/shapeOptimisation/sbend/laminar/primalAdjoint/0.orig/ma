FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      ma;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -3 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    frontBack
    {
        type            empty;
    }

    sideWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    upper
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    Inlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    Outlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    lower
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


