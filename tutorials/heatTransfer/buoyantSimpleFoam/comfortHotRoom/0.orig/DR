FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      DR;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            calculated;
        value           uniform 0;
    }
    inlet
    {
        type            calculated;
        value           uniform 16.61;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
}


