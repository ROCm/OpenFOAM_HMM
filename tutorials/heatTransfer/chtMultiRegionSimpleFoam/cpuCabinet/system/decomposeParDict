FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains  10;

//method          assembleDecomp;
// method          hierarchical;
// method          simple;
// method          manual;
method  scotch;

constraints
{
    faces
    {
        type    preserveFaceZones;
        //zones   (f1 f2 f3);
        zones (domain0_to_v_CPU domain0_to_v_fins v_CPU_to_v_fins);
        enabled true;
    }
}

