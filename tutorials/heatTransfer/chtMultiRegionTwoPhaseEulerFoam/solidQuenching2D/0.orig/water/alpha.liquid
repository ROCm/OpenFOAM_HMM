FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/water";
    object      alpha.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform 1;

boundaryField
{
    maxY
    {
        type            calculated;
        value           uniform 1;
    }
    symmetry
    {
        type            symmetry;
    }
    maxX
    {
        type            calculated;
        value           uniform 1;
    }
    minY
    {
        type            calculated;
        value           uniform 1;
    }
    empty
    {
        type            empty;
    }
    water_to_solid
    {
        type            calculated;
        value           uniform 1;
    }
}


