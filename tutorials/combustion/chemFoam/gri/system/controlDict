FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     chemFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.07;

deltaT          1e-05;

maxDeltaT       1e-4;

adjustTimeStep  on;

writeControl    adjustable;

writeInterval   0.01;

purgeWrite      0;

writeFormat     ascii;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

DebugSwitches
{
    SolverPerformance   0;
}

functions
{
    sensitivityAnalysis
    {
        libs                (fieldFunctionObjects);
        type                psiReactionsSensitivityAnalysis;
        writeControl        writeTime;
    }
}


