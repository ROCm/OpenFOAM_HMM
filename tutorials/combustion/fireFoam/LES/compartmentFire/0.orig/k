FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1e-4;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    entrainment
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    region0_to_panelRegion_wallPanel
    {
        type            zeroGradient;
    }

    region0_to_panelRegion_internalWallPanel_top
    {
        type            zeroGradient;
    }

    region0_to_panelRegion_internalWallPanel_bottom
    {
        type            zeroGradient;
    }
}


