FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 6;

method          hierarchical;

coeffs
{
    n           (6 1 1);
}

// Optional decomposition constraints
constraints
{
    preservePatches
    {
        type            preservePatches;
        patches         (cyclic0 cyclic1 ami0 ami1);
    }
}


