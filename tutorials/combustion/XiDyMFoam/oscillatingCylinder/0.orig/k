FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    "(walls|cylinder)"
    {
        type            kqRWallFunction;
        value           uniform 1;
    }

    "inlet.*"
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.04;
        value           uniform 1;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           uniform 1;
    }

    frontAndBack
    {
        type            empty;
    }
}


