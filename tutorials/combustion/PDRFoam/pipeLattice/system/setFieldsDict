FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue p       100000
    volScalarFieldValue T       287
    volScalarFieldValue Tu      287
    volScalarFieldValue ft      0
);


regions
(
    boxToCell
    {
        box    (0 0 0) (4.0 4.0 2.0);

        fieldValues
        (
            volScalarFieldValue ft 0.0550457705582192
        );
    }
);


