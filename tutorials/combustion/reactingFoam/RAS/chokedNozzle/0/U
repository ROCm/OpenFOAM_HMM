FoamFile
{
    version         2;
    format          ascii;
    class           volVectorField;
    location        "0";
    object          U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform (0 0 0);

boundaryField
{
    inletair
    {
        type            flowRateInletVelocity;
        value           $internalField;
        massFlowRate    csvFile;
        nHeaderLine     1;
        refColumn       0;
        componentColumns ( 1 );
        separator       ",";
        mergeSeparators no;
        file            "<constant>/inlet.csv";
    }
    outlet
    {
        type            pressureInletOutletVelocity;
        inletValue      $internalField;
        value           $internalField;
    }
    walls
    {
        type            fixedValue;
        value           $internalField;
    }
}


// ************************************************************************* //
