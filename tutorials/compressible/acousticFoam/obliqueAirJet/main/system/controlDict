FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     acousticFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.03;

deltaT          1e-6;

writeControl    timeStep;

writeInterval   200;

purgeWrite      0;

writeFormat     binary;

writePrecision  12;

timeFormat      general;

timePrecision   12;

runTimeModifiable yes;

adjustTimeStep no;


