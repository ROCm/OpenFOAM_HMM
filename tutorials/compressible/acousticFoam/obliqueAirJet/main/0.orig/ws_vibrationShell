FoamFile
{
    version     2.0;
    format      ascii;
    class       areaScalarField;
    object      wS;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions     [0 1 0 0 0 0 0];

internalField      uniform 0.0;

boundaryField
{
    sealing
    {
        type       clampedPlate;
        value      $internalField;
    }
}


