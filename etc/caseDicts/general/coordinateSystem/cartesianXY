FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      cartesianXY;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type      cartesian;

origin    (0 0 0);

rotation
{
    type  axes;
    e1    $x;
    e2    $y;
}

