FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "templates";
    object      wallOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

wallFunction
{
    highReynolds
    {
        nut
        {
            type        nutkWallFunction;
            value       ${:VALUE.nut};
        }
    }
    lowReynolds
    {
        nut
        {
            type        fixedValue;
            value       uniform 0;
        }
    }
}


