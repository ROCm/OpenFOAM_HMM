#!/bin/sh
cd ${0%/*} || exit 1    # run from this directory
set -x

wmake blockMesh
wmake all extrude

extrude2DMesh/Allwmake

wmake snappyHexMesh

if [ -d "$CGAL_ARCH_PATH" ]
then
    cvMesh/Allwmake
    cv2DMesh/Allwmake
fi

# ----------------------------------------------------------------- end-of-file
