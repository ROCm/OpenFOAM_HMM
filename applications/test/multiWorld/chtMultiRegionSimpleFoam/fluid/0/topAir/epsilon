FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/topAir";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -3 0 0 0 0 ];

internalField   uniform 0.01;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    left
    {
        type            fixedValue;
        value           $internalField;
    }
    right
    {
        type            inletOutlet;
        value           $internalField;
        inletValue      $internalField;
    }
    top
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    bottom
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
}


// ************************************************************************* //
