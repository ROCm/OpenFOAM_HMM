x
(
    0
    0.25
    0.5
    0.75
    1
);


function1 table
(
    (0 0)(10 1)
);

function2
{
    type        expression;
    expression  #{ sqr(arg()) #};
}

function2b expression;

function2bCoeffs
{
    type        expression;
    expression  #{ sqr(arg()) #};
}


